{
  "name": "cdk-multi-env",
  "version": "1.0.0",
  "bin": {
    "cdk-multi-env": "bin/cdk-multi-env.js"
  },
  "scripts": {
    "cdk": "cdk",
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w",
    "deploy": "cross-env-shell bash ./deploy.sh",
    "destroy": "cross-env-shell bash ./destroy.sh",
    "deploy:dev": "source .env.develop && cdk deploy --profile $AWS_PROFILE",
    "deploy:prod": "source .env.main && cdk deploy --profile $AWS_PROFILE",
    "destroy:dev": "source .env.develop && cdk destroy --profile $AWS_PROFILE",
    "destroy:prod": "source .env.main && cdk destroy --profile $AWS_PROFILE"
  },
  "dependencies": {
    "@rkesters/git-agent": "^1.0.2",
    "aws-cdk-lib": "2.148.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "aws-cdk": "2.148.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  }
}
